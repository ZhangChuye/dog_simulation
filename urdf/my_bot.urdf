            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0" />
            <mass value="1" />
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
        </inertial>
    </link>

    <joint name = "joint_b_r1" type = "revolute">
        <parent link = "body"/>
        <child link = "right_link_1"/>
        <origin xyz="0 -0.125 0" />
        <axis xyz="0 1 0" />
