            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0" />
            <mass value="1" />
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
        </inertial>
    </link>

    <joint name = "joint_r2_r3" type = "prismatic">
        <parent link = "right_link_2"/>
