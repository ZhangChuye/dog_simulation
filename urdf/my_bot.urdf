            </geometry>
            <material name="White"/>
        </visual>

        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.075"/>
            <geometry>
                <cylinder length="0.15" radius="0.03"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0" />
            <mass value="2" />
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
        </inertial>
    </link>

    <joint name = "joint_r1_r2" type = "revolute">
        <parent link = "right_link_1"/>
        <child link = "right_link_2"/>
        <origin xyz="0 -0.025 0" />
        <axis xyz="1 0 0" />
        <limit effort = "300" lower = "-3.39" upper = "0.25" velocity = "1"/>
    </joint>

<!-- /    Right Link3    / -->
    <link name = "right_link_3">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.15"/>
            <geometry>
                <cylinder length="0.3" radius="0.02"/>
            </geometry>
